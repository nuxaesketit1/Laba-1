#include <iostream>
#include <string>
using namespace std;
int main()
{ 
    double a,b; 
       
    cout << "Enter numbers a and b : " << endl;
    cin >>a>>b;
    
    a*=a;
    b*=b;
    
    cout << "Sum of squares :" << a+b << endl;
    cout << "Square difference :" << a-b << endl;
    cout << "Product of squares :" << a*b << endl;
    cout << "Private squares :" << a/b << endl;

     return 0; 
}
